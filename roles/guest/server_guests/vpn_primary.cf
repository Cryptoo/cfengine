body file control

{

  namespace => "cryptoo";

}

bundle agent vpn_primary(root)

{

  meta:

    "tags" slist => { "cryptoo_role" };

  methods:

    any::

      "configure system as the primary VPN client router"
        usebundle => server_guest("$(this.root)", "vpn_primary", "x86_64", "no_multilib");

}
