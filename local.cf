body file control

{

  namespace => "cryptoo";

  inputs => {
              "cryptoo/control.cf",
              "@(cryptoo:control.local_inputs)"
            };

}

bundle agent local

{
  vars:

    any::

      "bundles"
        slist => bundlesmatching("local:.*", "cryptoo_auto");

  methods:

    any::

      "run local bundles"
        usebundle => $(bundles);

}
