body file control

{

  namespace => "cryptoo";

  inputs => {
              "cryptoo/guest/server_guests/server_guest.cf"
            };

}

bundle agent tor

{

  meta:

    "tags" slist => { "cryptoo_role" };

  methods:

    any::

      "configure system as a Tor node"
        usebundle => server_guest("/", "tor", "x86_64");

}
