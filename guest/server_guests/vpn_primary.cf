body file control

{

  namespace => "cryptoo";

  inputs => {
              "cryptoo/guest/server_guests/server_guest.cf"
            };

}

bundle agent vpn_primary

{

  meta:

    "tags" slist => { "cryptoo_role" };

  methods:

    any::

      "configure system as the primary VPN client router"
        usebundle => server_guest("/", "vpn_primary", "x86_64");

}