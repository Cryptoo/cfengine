body file control

{

  namespace => "cryptoo";

}

bundle agent gateway_interface(interface, type)

{

  classes:

    any::

      "type_$(this.type)"
        expression => "any";

    type_gateway|type_dhcp::

      "gateway"
        expression => "any";

  reports:

    gateway::

      "$(interface)"
        bundle_return_value_index => "$(interface)";

}