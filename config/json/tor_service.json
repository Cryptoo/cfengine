             { "LHS": "KillSignal", "RHS": "SIGINT" },
             { "LHS": "TimeoutStopSec", "RHS": "32" },
             { "LHS": "LimitNOFILE", "RHS": "30000" }
           ]
       },
       { "section": "Install",
         "values":
