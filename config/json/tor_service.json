             { "LHS": "ExecStartPre", "RHS": "/usr/bin/tor --verify-config -f /etc/tor/torrc" },
             { "LHS": "ExecStart", "RHS": "/usr/bin/tor --RunAsDaemon 0 -f /etc/tor/torrc" },
